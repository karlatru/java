pipeline{
  agent any
  stages {
      stage('build'){
          steps{
          echo 'Hi TCS app is being Build....'
          
          }
      }
      stage('test'){
          steps{
           echo 'Hi TCS app is being tested....'
          
          }
      }
      stage('deploy'){
          steps{
           echo 'Hi TCS app is being deployed....'
          }
      }
  }
}
